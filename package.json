{
  "name": "litegraph-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix && npm run format",
    "test": "jest   --bail  --transformIgnorePatterns 'node_modules/(?!react-hot-toast)/'",
    "test:watch": "npm run test -- -watch",
    "test:update": "npm run test -- -u",
    "test:coverage": "jest --transformIgnorePatterns 'node_modules/(?!react-hot-toast)/' --coverage",
    "test:ci": "jest --transformIgnorePatterns 'node_modules/(?!react-hot-toast)/' --ci --coverage --maxWorkers=2",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.23.0",
    "@ant-design/icons": "^5.3.1",
    "@ant-design/nextjs-registry": "^1.0.2",
    "@react-sigma/core": "^3.4.2",
    "@reduxjs/toolkit": "^2.2.1",
    "@types/file-saver": "^2.0.7",
    "@types/react-resizable": "^3.0.8",
    "@xmldom/xmldom": "^0.9.8",
    "antd": "^5.19.1",
    "classnames": "^2.5.1",
    "eslint-config-next": "^14.2.5",
    "file-saver": "^2.0.5",
    "graphology": "^0.25.4",
    "graphology-types": "^0.24.7",
    "jsoneditor-react": "^3.1.2",
    "litegraphdb": "^1.0.5-beta.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.2.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-redux": "^9.1.2",
    "react-resizable": "^3.0.5",
    "sass": "^1.83.0",
    "sigma": "^2.4.0",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.4.9",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "overrides": {
    "jsoneditor-react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
